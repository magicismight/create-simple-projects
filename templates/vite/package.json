  },
  "keywords": [
    "<%- KEYWORDS.join('\", \"') %>"
  ],
  "license": "ISC",
  "devDependencies": {
    "vite": "^2.8.0",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-alloy": "^4.2.0",
    "husky": "4.3.8",
    "jest": "^27.0.6",
    "lint-staged": "9.4.3",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "npm-run-all": "^4.1.5"
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
