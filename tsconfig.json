{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": false,
    "allowJs": false,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "templates"
  ]
}
